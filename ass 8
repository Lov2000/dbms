SELECT *FROM  EMP_RECORD WHERE DOJ='03-Jan-2017 ';
 
 SELECT EMPID,ENAME,SALARY,to_char (DOJ,'MONTH DD,YYYY') FROM  EMP_RECORD;
 
 SELECT * from  EMP_RECORD WHERE DOJ<('03-Jan-2018')
 
 SELECT * from  EMP_RECORD WHERE TO_CHAR(DOJ,'MON')='JAN';
 
  SELECT * FROM EMP_RECORD WHERE to_char(DOJ, 'YYYY')= 2019;
	
	SELECT *FROM EMP_RECORD ORDER BY DOJ DESC; 
	
	SELECT *FROM EMP_RECORD WHERE mod(salary,2) = 1;
