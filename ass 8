CREATE TABLE EMP_RECORD(EMPID VARCHAR2(3)  , ENAME VARCHAR2(25) NOT NULL,
DOB DATE NOT NULL,DOJ DATE NOT NULL,SALARY NUMBER(7) NOT NULL ,
PRIMARY KEY(EMPID),
CONSTRAINT CK1 CHECK(EMPID LIKE 'E%'),
CONSTRAINT CK2 CHECK(SALARY>0),
CONSTRAINT CK3 CHECK(DOB>DOJ));

